    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "audio-metadata": "^0.3.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.1.3",
    "chakra-player": "^1.1.10",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "downloadjs": "^1.4.7",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "firebase": "^9.9.0",
    "fluent-ffmpeg": "^2.1.2",
    "framer-motion": "^6.4.3",
    "http": "^0.0.1-security",
    "node-id3": "^0.2.6",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-modern-audio-player": "^1.4.0-rc.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sass": "^1.53.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.4.1",
    "typescript": "^4.4.2",
    "uuid": "^9.0.0",
