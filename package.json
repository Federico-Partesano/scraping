{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "build": {
    "productName": "music-youtube",
    "appId": "music-youtube",
    "win": {
      "icon": "logo.ico",
      "target": "nsis",
      "publish": false
  }
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
